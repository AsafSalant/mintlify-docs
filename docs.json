        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Crawls",
            "pages": [
              "api-reference/endpoint/crawls/get-crawl",
              "api-reference/endpoint/crawls/create",
